<Window x:Class="TSS.Wpf.WindowUniverseSizeGetter"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TSS.Wpf"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        ResizeMode="NoResize"
        Height="148.872" Width="242.965" Style="{DynamicResource defStyle}" KeyDown="Window_KeyDown">
    <Grid>
        <Label x:Name="labelWidth" Content="Width" HorizontalAlignment="Left" Margin="10,11,0,0" VerticalAlignment="Top"/>
        <Label x:Name="labelHeight" Content="Height" HorizontalAlignment="Left" Margin="10,41,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBoxWidth" Height="23" Margin="76,18,21,0" TextWrapping="Wrap" Text="100" VerticalAlignment="Top"/>
        <TextBox x:Name="textBoxHeight" Height="23" Margin="76,46,21,0" TextWrapping="Wrap" Text="100" VerticalAlignment="Top"/>
        <Button x:Name="buttonOk" Content="OK" HorizontalAlignment="Left" Margin="10,76,0,0" VerticalAlignment="Top" Width="50" Click="buttonOk_Click"/>
        <Button x:Name="buttonCancel" Content="Cancel" HorizontalAlignment="Left" Margin="141,76,0,0" VerticalAlignment="Top" Width="75" Click="buttonCancel_Click"/>

    </Grid>
</Window>
